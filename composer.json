        "guzzlehttp/guzzle": "^6.2 | ^7",
        "league/oauth2-client": "^2.3"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7@stable",
        "silverstripe/sqlite3": "^2.1"
    },
    "autoload": {
        "psr-4": {
